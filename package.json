{
  "engines": {
    "node": ">=14.16.1"
  },
  "dependencies": {
    "@swc/cli": "^0.1.39",
    "@swc/core": "^1.2.54",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "esbuild": "^0.12.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.3",
    "typescript": "^4.2.4",
    "webpack": "^5.39.0",
    "webpack-cli": "^4.7.2"
  },
  "scripts": {
    "tsc": "npm run build:tsc && node builds/tsc/main.js",
    "build:tsc": "tsc --outDir builds/tsc",
    "js": "node javascript/main.js",
    "build:swc": "swc src -d builds/swc --config-file .swcrc",
    "swc": "npm run build:swc && node builds/swc/main.js",
    "build:esbuild": "node esbuild.js",
    "esbuild": "npm run build:esbuild && node builds/esbuild/main.js",
    "build:webpack": "webpack",
    "webpack": "npm run build:webpack && node builds/webpack/main.js",
    "build:all": "rimraf ./builds && npm run build:tsc && npm run build:swc && npm run build:esbuild && npm run build:webpack"
  }
}
